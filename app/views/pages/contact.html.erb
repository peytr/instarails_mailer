<div class="container">
<br>
    <h1>Contact Us</h1>

    <%= form_for :contact do |f| %>
        <p>
        <%= f.label :name %>
        <br>
        <%= f.text_field :name %>
        </p>
        <p>
        <%= f.label :message %>
        <br>
        <%= f.text_area :message %>
        </p>
        <p>
        <%= f.submit %>
        </p>
    <% end %>
</div>